{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$assets/*": ["assets/*"],
      "$api/*": ["src/api/*"],
      "$components/*": ["src/components/*"],
      "$consts/*": ["src/consts/*"],
      "$context": ["src/context/index.ts"],
      "$context/*": ["src/context/*"],
      "$screens/*": ["src/screens/*"],
      "$styles": ["src/components/styles.ts"],
      "$types/*": ["src/types/*"],
      "$utils/*": ["src/utils/*"]
    },
    "jsx": "react-native"
  },
  "extends": "expo/tsconfig.base"
}
