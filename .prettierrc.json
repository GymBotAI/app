{
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "<TYPES>^react",
    "<TYPES>^react-native",
    "<TYPES>^expo-.+",
    "<TYPES>",
    "<TYPES>^[.]",
    "",
    "^react$",
    "^react-native$",
    "^expo-.+$",
    "",
    "^[$]",
    "^[.]",
    "",
    "<BUILTIN_MODULES>",
    "",
    "<THIRD_PARTY_MODULES>"
  ],
  "importOrderTypeScriptVersion": "5.3.3"
}
