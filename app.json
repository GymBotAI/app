{
  "expo": {
    "name": "GymBot",
    "slug": "gymbot",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo-square.png",
    "scheme": "gymbot",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-square.png",
      "resizeMode": "contain",
      "backgroundColor": "#0079f3"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["gymbot"]
      },
      "bundleIdentifier": "com.gymbot.gymbot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-square.png",
        "backgroundColor": "#0079f3",
        "__comment": "TODO: make a valid adaptive icon"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-square.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bba80b0b-b031-4d39-a729-eba9f033bfb2"
      }
    },
    "owner": "gymbot"
  }
}
